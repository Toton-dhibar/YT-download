{
  "functions": {
    "api/proxy.js": {
      "maxDuration": 25
    }
  },
  "rewrites": [
    { "source": "/xhttp/:path*", "destination": "/api/proxy" }
  ],
  "headers": [
    {
      "source": "/xhttp/:path*",
      "headers": [
        {
          "key": "X-Proxy-By",
          "value": "Vercel-CDN"
        }
      ]
    }
  ]
}
